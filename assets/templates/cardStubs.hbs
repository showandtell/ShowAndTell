{{#each cards}}
  <div
    data-idx="{{@index}}"
    class="card-label {{#isCurrent}}current{{/isCurrent}}">
      {{#if previewImageURL}}
          <img src="{{previewImageURL}}"></img>
      {{/if}}
      {{#if text}}
          <div class="label-text">{{text}}</div>
      {{/if}}
      <div class="label-handle"></div>
  </div>
{{/each}}